{{ 'section-collage-builder.min.css' | asset_url | stylesheet_tag: preload: true }}
<noscript>{{ 'section-collage-builder.min.css' | asset_url | stylesheet_tag }}</noscript>

{% if section.blocks.size > 0 %}
  {%- liquid
    if section.settings.section_padding_top == 0
      assign padding_section_top = 0.1
    else
      assign padding_section_top = section.settings.section_padding_top
    endif
    if section.settings.section_padding_bottom == 0
      assign padding_section_bottom = 0.1
    else
      assign padding_section_bottom = section.settings.section_padding_bottom
    endif

    if section.settings.section_width == 'full' and section.settings.remove_spacing
      assign section_has_no_horizontal_spacing = true
    endif
  -%}
<section 
  class="wrapper-spacing--v{% unless section_has_no_horizontal_spacing %} wrapper-spacing--h{% endunless %} color-scheme--{{ section.settings.color_scheme }} section--{{ section.id }}"
  data-wetheme-section-type="collage-builder" 
  data-wetheme-section-id="{{ section.id }}"
  style="--padding-section-top: {{ padding_section_top }}px; --padding-section-bottom: {{ padding_section_bottom }}px;"
>

  {% comment %} Section title and description {% endcomment %}
  {% render 'dynamic-section-header' with section_title: section.settings.title, section_description: section.settings.description %}

  <div class="{% if section.settings.section_width == 'content' %} width--content{% endif %}{% if section.settings.remove_spacing %} collage-builder--no-spacing{% endif %}{% if section.settings.title != blank or section.settings.description != blank %} collage-builder--with-header{% endif %}">
    <div class="collage-builder__grid-container collage-builder__grid-container--{{ section.id }}">
      {% for block in section.blocks %}
        {% assign timer_complete_attrs = '' %}
        {% if block.type == 'countdown_timer' %}
          {% capture timer_complete_attrs %}
            data-timer-complete-content data-timer-id={{ block.id }}
          {% endcapture %}
        {% endif %}

        {% capture text_content %}
          <div class="collage-builder__text-container collage-builder__text-container--{{ block.id }} {{ block.settings.desktop_position }} {{ block.settings.mobile_position }}">
            <div class="collage-builder__text-container--inner">
              {% if block.settings.heading != blank %}
                <h2 class="collage-builder__heading" {{ timer_complete_attrs }}>{{ block.settings.heading }}</h2>
              {% endif %}
              {% if block.settings.timer_complete_message != blank %}
                <h2 class="collage-builder__heading hide" {{ timer_complete_attrs }}>{{ block.settings.timer_complete_message }}</h2>
              {% endif %}
              {% if block.type == 'countdown_timer' %}
                <div class="collage-builder__timer">
                  {% render 'countdown-timer' with
                    timer_id: block.id,
                    end_date_setting: block.settings.end_date,
                    end_time_setting: block.settings.end_time,
                    hide_when_complete: false,
                    time_labels: block.settings.time_labels,
                    is_block: true,
                    background_color_override: block.settings.text_color
                  %}
                </div>
              {% endif %}
              {% if block.settings.text != blank %}
                <p class="collage-builder__text-content text-link-animated" {{ timer_complete_attrs }}>{{ block.settings.text }}</p>
              {% endif %}
              {% if block.settings.html != blank %}
                <div class="collage-builder__text-content text-link-animated">{{ block.settings.html }}</div>
              {% endif %}
              {% if block.settings.button_label != blank and block.settings.url != blank %}
                {% liquid
                  case block.settings.buttons_style
                    when 'filled'
                      assign button_classes = 'btn'
                    when 'outlined'
                      assign button_classes = 'btn btn--secondary'
                    when 'underlined'
                      assign button_classes = 'btn btn--underlined'
                    when 'squared'
                      assign button_classes = 'btn--squared'
                  endcase
                %}
                <div class="collage-builder__button-container{% if block.settings.buttons_style == 'underlined' %} text-link-animated{% endif %}" style="background-color: {{ block.settings.button_container_background_color }};">
                  <a href="{{ block.settings.url }}" class="{% if block.settings.buttons_style == 'squared' %}btn--squared{% else %}{{ button_classes }}{% endif %} homepage-section-btn" style="color: {{ block.settings.button_text_color }};">
                    <span class="button-title">{{ block.settings.button_title }}</span>
                    <span class="button-description">{{ block.settings.button_description }}</span>
                    <span class="button-link">{{ block.settings.button_label }}</span>
                  </a>
                </div>
              {% endif %}
            </div>
          </div>
        {% endcapture %}

        {% case block.type %}
          {% when 'image' %}
            {% capture block_content %}
              <div class="collage-builder__media-wrapper">
                <div class="collage-builder__image collage-builder__image--{{ block.id }}">
                  {% if block.settings.desktop_image != blank %}
                    <img src="{{ block.settings.desktop_image | img_url: 'master' }}" alt="{{ block.settings.desktop_image.alt }}" class="collage-builder__image" />
                  {% else %}
                    {% assign default_block_position = block.id | slice: -2, 2 %}
                    {% case default_block_position %}
                      {% when '-4' %}
                        {% assign placeholder_name = 'collection-1' %}
                      {% when '-5' %}
                        {% assign placeholder_name = 'lifestyle-1' %}
                      {% else %}
                        {% assign placeholder_name = 'image' %}
                    {% endcase %}
                    {{ placeholder_name | placeholder_svg_tag: 'icon icon--placeholder' }}
                  {% endif %}
                  {% if block.settings.mobile_image != blank %}
                    <img src="{{ block.settings.mobile_image | img_url: 'master' }}" alt="{{ block.settings.mobile_image.alt }}" class="collage-builder__image" />
                  {% endif %}
                </div>

                <div class="collage-builder__overlay"></div>

                <div class="collage-builder__text-container collage-builder__text-container--{{ block.id }} {{ block.settings.desktop_position }} {{ block.settings.mobile_position }}">
                  <div class="collage-builder__text-container--inner">
                    {% if block.settings.heading != blank %}
                      <h2 class="collage-builder__heading">{{ block.settings.heading }}</h2>
                    {% endif %}
                    {% if block.settings.button_label != blank and block.settings.url != blank %}
                      {% liquid
                        case block.settings.buttons_style
                          when 'filled'
                            assign button_classes = 'btn'
                          when 'outlined'
                            assign button_classes = 'btn btn--secondary'
                          when 'underlined'
                            assign button_classes = 'btn btn--underlined'
                          when 'squared'
                            assign button_classes = 'btn--squared'
                        endcase
                      %}
                      <div class="collage-builder__button-container{% if block.settings.buttons_style == 'underlined' %} text-link-animated{% endif %}" style="background-color: {{ block.settings.button_container_background_color }};">
                        <a href="{{ block.settings.url }}" class="{% if block.settings.buttons_style == 'squared' %}btn--squared{% else %}{{ button_classes }}{% endif %} homepage-section-btn" style="color: {{ block.settings.button_text_color }};">
                          <span class="button-title">{{ block.settings.button_title }}</span>
                          <span class="button-description">{{ block.settings.button_description }}</span>
                          <span class="button-link">{{ block.settings.button_label }}</span>
                        </a>
                      </div>
                    {% endif %}
                  </div>
                </div>
              </div>
            {% endcapture %}
          {% when 'background_video' %}
            {% assign block_aspect = section.settings[height_setting_id] | divided_by: section.settings[width_setting_id] %}
            {% if block_aspect > 1.78 %}
              {% assign block_aspect_type = 'wide' %}
            {% else %}
              {% assign block_aspect_type = 'narrow' %}
            {% endif %}

            {% capture block_content %}
              <div class="collage-builder__media-wrapper">
                {{ text_content }}
                <div class="collage-builder__background-video collage-builder__background-video--desktop collage-builder__background-video--{{ block_aspect_type }} type-{{ type }}">
                  {% if block.settings.video %}
                    {% render 'native-video' with video: block.settings.video, autoplay: true, loop: true, muted: true, overlay: false, controls: false %}
                  {% elsif block.settings.video_url %}
                    <wetheme-video
                      section-id="{{ block.id }}"
                      v-bind:hero="true"
                      video-type="{{ type }}"
                      url="{{ block.settings.video_url }}"
                      v-bind:autoplay="true"
                      v-bind:loop="true"
                      v-bind:mute="true"
                      v-bind:hide-controls="true"
                      v-bind:overlay="false"
                    >
                    </wetheme-video>
                  {% endif %}
                </div>
                {% if block.settings.mobile_video_url != blank or block.settings.mobile_video %}
                  <div class="collage-builder__background-video collage-builder__background-video--mobile type-{{ mobile_type }}">
                    {% if block.settings.mobile_video %}
                      {% render 'native-video' with video: block.settings.mobile_video, autoplay: true, loop: true, muted: true, overlay: false, controls: false %}
                    {% elsif block.settings.mobile_video_url %}
                      <wetheme-video
                        section-id="{{ block.id }}"
                        v-bind:hero="true"
                        video-type="{{ mobile_type }}"
                        url="{{ block.settings.mobile_video_url }}"
                        v-bind:autoplay="true"
                        v-bind:loop="true"
                        v-bind:mute="true"
                        v-bind:hide-controls="true"
                        v-bind:overlay="false"
                      >
                      </wetheme-video>
                    {% endif %}
                  </div>
                {% endif %}
                <div class="collage-builder__overlay"></div>
              </div>
            {% endcapture %}
          {% when 'video' %}
            {% capture block_content %}
              <div class="collage-builder__media-wrapper">
                <div class="collage-builder__video collage-builder__video--desktop type-{{ type }} block--{{ block.id }}">
                  {% if block.settings.video %}
                    {% render 'native-video' with video: block.settings.video, autoplay: false, loop: false, muted: false, overlay: true, controls: false, scope_id: block.id %}
                  {% elsif block.settings.video_url %}
                    <wetheme-video
                      section-id="{{ block.id }}"
                      v-bind:hero="false"
                      video-type="{{ type }}"
                      url="{{ block.settings.video_url }}"
                      v-bind:autoplay="false"
                      v-bind:loop="false"
                      v-bind:mute="false"
                      v-bind:overlay="true"
                    >
                    </wetheme-video>
                  {% endif %}
                </div>
                {% if block.settings.mobile_video_url != blank or block.settings.mobile_video %}
                  <div class="collage-builder__video collage-builder__video--mobile type-{{ mobile_type }}">
                    {% if block.settings.mobile_video %}
                      {% render 'native-video' with video: block.settings.mobile_video, autoplay: false, loop: false, muted: false, overlay: true, controls: false, scope_id: block.id %}
                    {% elsif block.settings.mobile_video_url %}
                      <wetheme-video
                        section-id="{{ block.id }}"
                        v-bind:hero="false"
                        video-type="{{ mobile_type }}"
                        url="{{ block.settings.mobile_video_url }}"
                        v-bind:autoplay="false"
                        v-bind:loop="false"
                        v-bind:mute="false"
                        v-bind:overlay="true"
                      >
                      </wetheme-video>
                    {% endif %}
                  </div>
                {% endif %}
              </div>
              <style>
                {% if block.settings.cover_image %}
                  {% capture selector %}.homepage-video-content-overlay-wrapper-{{ block.id }} .video--video__static-image{% endcapture %}
                  {% render 'responsive-background-image' with block.settings.cover_image, selector: selector %}
                {% endif %}
                {% if block.settings.button_background_color != 'rgba(0,0,0,0)' %}
                  .using-keyboard .collage-builder__video.block--{{ block.id }} .homepage-video-play-button:focus {
                    outline: 2px solid {{ block.settings.button_background_color }};
                  }
                  .section--{{ section.id }} .collage-builder__video.block--{{ block.id }} .homepage-video-play-button {
                    background-color: {{ block.settings.button_background_color }};
                  }
                {% endif %}
                {% if block.settings.button_icon_color != 'rgba(0,0,0,0)' %}
                  .section--{{ section.id }} .collage-builder__video.block--{{ block.id }} .homepage-video-play-button .homepage-video-play-button-inner {
                    border-color: transparent transparent transparent {{ block.settings.button_icon_color }};
                  }
                {% endif %}
              </style>
            {% endcapture %}
          {% when 'text' or 'countdown_timer' or 'html' %}
            {% capture block_content %}
              {{ text_content }}
              <style>
                .collage-builder__text-container--{{ block.id }} {
                  text-align: {{ block.settings.text_alignment }};
                  background-color: {{ block.settings.background_color }};
                }
                {% if block.type == 'countdown_timer' %}
                  {% case block.settings.text_alignment %}
                    {% when 'left' %}
                      {% assign flex_align = 'flex-start' %}
                    {% when 'center' %}
                      {% assign flex_align = 'center' %}
                    {% when 'right' %}
                      {% assign flex_align = 'flex-end' %}
                  {% endcase %}
                  .collage-builder__text-container--{{ block.id }} .collage-builder__timer {
                    justify-content: {{ flex_align }}
                  }
                {% endif %}
              </style>
            {% endcapture %}
        {% endcase %}
        
        <div class="collage-builder__grid-item collage-builder__grid-item--{{ section.id }}--{{ forloop.index }} collage-builder__grid-item--{{ block.type }} collage-builder__grid-item--{{ block.id }}">
          <div class="collage-builder__grid-item--inner">
            <div class="collage-builder__content">
              {{ block_content }}
            </div>
          </div>
        </div>
        
        <style>
.collage-builder__button-container{
  border-top-left-radius: 10px;
  border-bottom-left-radius: 10px;
}
          @media screen and (min-width: 768px) {
            .collage-builder__grid-item--{{ section.id }}--{{ forloop.index }} {
              width: calc(100% / 6);
              padding-bottom: calc(100% / 6);
            }
          }

          @media screen and (max-width: 767px) {
            .collage-builder__grid-item--{{ section.id }}--{{ forloop.index }} {
              padding-bottom: 0;
            }
          }

          .collage-builder__text-container--{{ block.id }} {
            color: {{ block.settings.text_color }};
          }

          {% if block.settings.button_color != 'rgba(0,0,0,0)' %}
            .collage-builder__grid-item--{{ block.id }} .collage-builder__button {
              color: {{ block.settings.button_color }} !important;
              {% if block.settings.buttons_style == 'underlined' %}
                --link-border-color: {{ block.settings.button_color | color_modify: 'alpha', 0.50 }} !important;
                --color-scheme-text-color: {{ block.settings.button_color }};
                --color-scheme-text-color-transparent50: {{ block.settings.button_color | color_modify: 'alpha', 0.50 }};
              {% elsif block.settings.buttons_style == 'outlined' %}
                border: 1px solid {{ block.settings.button_color }} !important;
              {% endif %}
            }
          {% endif %}

          {% if block.settings.button_background_color != 'rgba(0,0,0,0)' and block.settings.buttons_style == 'filled' %}
            .collage-builder__grid-item--{{ block.id }} .collage-builder__button {
              background-color: {{ block.settings.button_background_color }} !important;
            }

            .collage-builder__grid-item--{{ block.id }} .collage-builder__button:focus {
              outline: 2px solid {{ block.settings.button_background_color }} !important;
            }
          {% endif %}

          {% unless block.settings.overlay_color == 'rgba(0,0,0,0)' %}
            .collage-builder__grid-item--{{ block.id }} .collage-builder__overlay {
              background: -webkit-linear-gradient(top, {{ block.settings.overlay_color | color_modify: 'alpha', 0.3 }} 0%, {{ block.settings.overlay_color | color_modify: 'alpha', 0.8 }} 75%);
              background: linear-gradient(to bottom, {{ block.settings.overlay_color | color_modify: 'alpha', 0.3 }} 0%, {{ block.settings.overlay_color | color_modify: 'alpha', 0.8 }} 75%);
            }
          {% endunless %}
        </style>
      {% endfor %}
    </div>
  </div>
</section>

<style>
  .collage-builder__grid-container--{{ section.id }} {
    display: grid;
    grid-template-columns: repeat(6, 1fr); /* 6 blocks in a row for large screens */
    gap: 10px; /* Adjust the gap between blocks as needed */
  }

  @media screen and (max-width: 1350px) {
    .collage-builder__grid-container--{{ section.id }} {
      grid-template-columns: repeat(3, 1fr); /* 3 blocks in a row for medium screens */
    }
  }

  @media screen and (max-width: 768px) {
    .collage-builder__grid-container--{{ section.id }} {
      grid-template-columns: repeat(2, 1fr); /* 2 blocks in a row for small screens */
    }
  }

  @media screen and (max-width: 480px) {
    .collage-builder__grid-container--{{ section.id }} {
      grid-template-columns: 1fr; /* 1 block in a row for very small screens */
    }
    .collage-builder__button-container{
      width: none;
    }
    .button-description {
      width: 100%;
    }
  }

  .collage-builder__media-wrapper:hover .collage-builder__overlay {
    cursor: pointer;
    transition-duration: 0.4s;
  }

  .collage-builder__text-container--{{ block.id }} {
    text-align: {{ block.settings.text_alignment }};
    background-color: {{ block.settings.background_color }};
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
  }

  .collage-builder__text-container--{{ block.id }} .collage-builder__text-container--inner {
    max-width: 80%;
    text-align: center;
  }

  .collage-builder__button-container {
    display: inline-block;
    width: 100%;
    max-width: 300px;
  }

  .collage-builder__button-container .btn {
    display: block;
    width: 100%;
    padding: 20px;
    text-align: center;
    font-size: 16px;
    color: {{ block.settings.button_color }};
    background-color: {{ block.settings.button_background_color }};
  }

  .collage-builder__button-container .btn--outlined {
    border: 1px solid {{ block.settings.button_color }};
  }

  .collage-builder__button-container .btn--underlined {
    border-bottom: 1px solid {{ block.settings.button_color }};
  }

  .collage-builder__button-container .btn--squared {
    display: block;
    width: 100%;
    padding: 10px 20px 10px 20px;
    text-align: center;
    font-size: 16px;
    color: {{ block.settings.button_color }};
    background-color: {{ block.settings.button_background_color }};
    border-radius: 0;
  }

  .button-title {
    display: block;
    font-size: 1.2em;
    font-weight: bold;
  }

  .button-description {
    display: block;
    font-size: 0.9em;
    margin-top: 5px;
  }

  .button-link {
    display: block;
    font-size:  0.8em;
    margin-top: 5px;
    text-decoration: underline;
    font-weight: 700;
  }
  .homepage-featured-box-btn, .homepage-section-btn{
    margin-top: 0px;
  }
@media screen and (max-width: 414px) and (min-width: 400px) {
    .collage-builder__button-container {
        left: 22% !important;
    }
}
@media screen and (max-width: 430px) and (min-width: 414px) {
    .collage-builder__button-container {
        left: 25% !important;
    }
}
@media screen and (max-width: 480px) and (min-width: 430px) {
    .collage-builder__button-container {
        left: 33% !important;
    }
}
@media screen and (max-width: 1348px) and (min-width: 1266px) {
    .collage-builder__button-container {
        left: 26% !important;
    }
}
  /* @media screen and (max-width: 468px) {
  .collage-builder__button-container{
    left: 25% !important;
  }
} */
  /* @media screen and (max-width: 768px) {
  .collage-builder__button-container{
    left: 22% !important;
  }
} */
</style>

{% endif %}

{% schema %}
{
